PROGRAM NUMERO_PRIMO3;
USES CRT;
VAR
  ES_PRIMO: BOOLEAN;
  I,N: WORD;
BEGIN
  CLRSCR;
  WRITE('N= ');
  READLN(N);

  ES_PRIMO:=TRUE;

  FOR I:=2 TO N-1 DO
    IF N MOD I = 0 THEN
	   ES_PRIMO:=FALSE;

  IF ES_PRIMO THEN WRITELN('ES PRIMO')

  ELSE WRITELN('NO ES PRIMO');

  REPEAT UNTIL KEYPRESSED;
END.